<script>
window.addEventListener('DOMContentLoaded', (event) => {
	const	select = document.querySelector('#smart-button-container #item-options');
	if (select) {
		let	selected = (new URL(location.href)).search;
		if (selected) {
			const	tbl = {1:'Donation 1 USD', 3:'Donation 3 USD', 5:'Donation 5 USD', 10:'Donation 10 USD'};
			selected = decodeURI(selected.replace('?p=', '').trim());
			select.value = tbl[selected] || 'Donation 1 USD';
		}
	}

  initPayPalButton();
});
</script>
